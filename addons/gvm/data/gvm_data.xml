<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for purchase.order --> 
        <record id="seq_sign_number" model="ir.sequence">
            <field name="name">Sign.Number</field>
            <field name="code">gvm.sign.number</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data noupdate="1">
        <!-- Sequences for purchase.order --> 
        <record id="seq_signline_number" model="ir.sequence">
            <field name="name">Signline.Number</field>
            <field name="code">gvm.signline.number</field>
            <!--prefix: 문서앞쪽 이름설정, padding:숫자앞0의갯수-->
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
